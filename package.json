{
  "name": "vizsurvey",
  "version": "1.1",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.10.9",
    "@mui/styles": "^5.11.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "aws-sdk": "^2.1135.0",
    "calendar-matrix": "^1.0.0",
    "chalk": "^5.2.0",
    "csvtojson": "^2.0.10",
    "cypress-real-events": "^1.7.1",
    "d3": "^7.4.4",
    "env-cmd": "^10.1.0",
    "firebase": "^9.18.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.4.0",
    "formik": "^2.2.9",
    "immer": "^8.0.1",
    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.4.5",
    "react-scripts": "5.0.1",
    "to-csv": "^0.1.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "kill": "kill -9 $(ps | grep -e 'node\\|npm' | sed -e 's/^\\([0-9]*\\).*/\\1/')",
    "cypress-open": "./node_modules/.bin/cypress open",
    "cypress-run": "./node_modules/.bin/cypress run --spec cypress/integration/vizsurvey.spec.js --browser chrome",
    "cypress-auto": "npm run start & cd test && npm start & npm run cypress-run && npm run kill",
    "format": "./node_modules/prettier/bin-prettier.js --write '**/*.js' '**/*.jsx'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-flow": "^6.23.0",
    "cypress": "^10.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-google": "^0.14.0",
    "flow-bin": "^0.158.0",
    "gh-pages": "^3.2.3",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "2.2.2",
    "prettier": "^2.3.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.1"
  },
  "jest": {
    "moduleNameMapper": {
      "d3": "<rootDir>/node_modules/d3/dist/d3.min.js"
    },
    "transformIgnorePatterns": [
      "/dist/.+\\.js"
    ],
    "globalSetup": "<rootDir>/src/dotenv-test.js"
  }
}
