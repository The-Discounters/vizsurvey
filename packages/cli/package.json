{
  "name": "cli",
  "version": "1.0.0",
  "description": "Command line interface for downloading and splitting Vizsurvey data files.",
  "main": "index.js",
  "type": "module",
  "bin": {
    "dsc": "./src/index.js"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "npm run test -- --watch",
    "publish": "npm install -g",
    "datarefresh": "pwsh ../scripts/refresh_exp_config.ps1"
  },
  "keywords": [
    "CLI",
    "Amazon",
    "S3"
  ],
  "author": "Peter N. Cordone",
  "license": "ISC",
  "dependencies": {
    "@the-discounters/types": "^1.0.0",
    "@the-discounters/util": "^1.0.0",
    "aws-sdk": "^2.1295.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "cli-color": "^2.0.3",
    "clui": "^0.3.6",
    "commander": "^9.4.1",
    "configstore": "^6.0.0",
    "d3": "^7.8.5",
    "dotenv": "^16.0.3",
    "figlet": "^1.5.2",
    "inquirer": "^9.1.4",
    "is-valid-path": "^0.1.1",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "to-csv": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@types/csvtojson": "^2.0.0",
    "@types/fs-extra": "^11.0.1",
    "babel-jest": "^29.4.3",
    "eslint": "^8.15.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "moduleNameMapper": {
      "testRegex": "((\\.|/*.)(test))\\.js?$"
    },
    "transformIgnorePatterns": [
      "/dist/.+\\.js",
      "/Users/pete/vizsurvey/node_modules/"
    ]
  }
}
