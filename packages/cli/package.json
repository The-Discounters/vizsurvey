    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "cli-color": "^2.0.4",
    "clui": "^0.3.6",
    "commander": "^12.1.0",
    "configstore": "^7.0.0",
    "csv-stringify": "^6.5.2",
    "d3": "^7.9.0",
